<Application x:Class="Stroitel.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Stroitel"
             StartupUri="Views/AuthStartupWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="ColorPrimary">#FFF</SolidColorBrush>
            <SolidColorBrush x:Key="ColorSecondary">#FFCC99</SolidColorBrush>
            <SolidColorBrush x:Key="ColorAccent">#CC6600</SolidColorBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>
